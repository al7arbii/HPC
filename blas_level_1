# Method for creating array of size from command line
function create_vector(n)
       vector_one = Vector(rand(1:50, n))
       vector_two = Vector(rand(1:50, n))
       return vector_one, vector_two
end

# General Vector-Vector
function gevv(x, y)
    result = 0.0
    for i in 1:length(x)
        result += x[i] * y[i]
    end
    return result
end

# Array creation
vector_two, vector_one = create_vector(10000)

# Dot product
A = gevv(vector_one, vector_two)
B = sum(vector_one .* vector_two)
println(isapprox(A, B, atol=1e-200000000))

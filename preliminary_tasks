# Method for creating array of size from command line
function create_array(n)
       array_one = Array(rand(1:50, n))
       array_two = Array(rand(1:50, n))
       return array_one, array_two
       end

# Array creation
array_one, array_two = create_array(5)
println(array_one)
println(array_two)

# Sum of arrays element wise
element_sum = zeros(Int, length(array_one))
for i in 1:length(array_one)
    element_sum[i] = array_one[i] + array_two[i]
end
println(element_sum)